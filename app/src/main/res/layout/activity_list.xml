<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.todayeat.MainActivity">

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp" />

    <Button
        android:id="@+id/buttonCharacter"
        android:layout_width="24dp"
        android:layout_height="24dp"


        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="58dp"
        android:layout_marginLeft="58dp"
        android:layout_marginBottom="21dp"
        android:background="@drawable/baseline_face_black_24dp" />

    <Button
        android:id="@+id/buttonHome"
        android:layout_width="36dp"
        android:layout_height="36dp"

        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="21dp"
        android:background="@drawable/baseline_home_black_36dp" />

    <Button
        android:id="@+id/buttonList"
        android:layout_width="24dp"
        android:layout_height="24dp"


        android:layout_alignTop="@+id/buttonHome"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="104dp"
        android:layout_marginLeft="104dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="61dp"
        android:layout_marginRight="61dp"
        android:layout_marginBottom="22dp"
        android:layout_toEndOf="@+id/buttonHome"
        android:layout_toRightOf="@+id/buttonHome"
        android:background="@drawable/baseline_menu_black_24dp" />

</RelativeLayout>
